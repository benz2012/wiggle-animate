{
  "name": "wiggle-animate",
  "version": "0.2.17",
  "private": true,
  "homepage": "/_subapps/animate",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/base": "^5.0.0-beta.29",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-only-warn": "^1.1.0",
    "jszip": "^3.10.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.3.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^3.4.3",
    "mp4-muxer": "^3.0.5",
    "overlayscrollbars": "^2.4.6",
    "overlayscrollbars-react": "^0.5.3",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.10",
    "uuid": "^9.0.1",
    "webm-muxer": "^3.2.1"
  },
  "scripts": {
    "update-fonts": "node ./scripts/getAllGoogleFonts.js",
    "start": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts start",
    "build": "REACT_APP_FONT_DATA_FILE=$(cd ./public && ls font-data-google*) REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts build",
    "test": "REACT_APP_TEST_ENV=true playwright test"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "react-app",
      "airbnb"
    ],
    "plugins": [
      "only-warn"
    ],
    "rules": {
      "no-console": "off",
      "max-len": [
        "warn",
        {
          "code": 120
        }
      ],
      "semi": [
        "warn",
        "never"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_.*",
          "argsIgnorePattern": "^_"
        }
      ],
      "no-underscore-dangle": "off",
      "lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ],
      "object-curly-newline": [
        "warn",
        {
          "multiline": true
        }
      ],
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ],
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-props-no-spreading": "off",
      "react/no-unknown-property": "off",
      "react/function-component-definition": "off",
      "react/prop-types": "off"
    }
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/node": "^20.11.20"
  }
}
